{
  "name": "autoslides",
  "productName": "AutoSlides",
  "version": "4.1.4",
  "description": "AutoSlides is an open-source desktop application that automates the creation of slideshows from images and videos. It offers a user-friendly interface, customizable templates, and supports various media formats to help users create engaging presentations effortlessly.",
  "main": ".vite/build/main.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make:mac": "electron-forge package && rm out/AutoSlides-darwin-arm64/LICENSE out/AutoSlides-darwin-arm64/LICENSES.chromium.html out/AutoSlides-darwin-arm64/version && echo \"sudo xattr -d com.apple.quarantine /Applications/AutoSlides.app\" > out/AutoSlides-darwin-arm64/install.command && chmod +x out/AutoSlides-darwin-arm64/install.command && dropdmg --config-name 'AutoSlides 2' out/AutoSlides-darwin-arm64",
    "make:win": "electron-forge package --platform=win32 --arch=x64 && electron-builder --win --config electron-builder.yml --prepackaged out/AutoSlides-win32-x64",
    "make:linux": "electron-forge package --platform=linux --arch=x64 && electron-builder --linux --config electron-builder.yml --prepackaged out/AutoSlides-linux-x64",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts,.tsx ."
  },
  "keywords": [],
  "author": {
    "name": "BIT-admin",
    "email": "info@ruc.edu.kg"
  },
  "homepage": "https://github.com/bit-admin/Yanhekt-AutoSlides",
  "license": "Apache-2.0",
  "devDependencies": {
    "@electron-forge/cli": "^7.10.2",
    "@electron-forge/plugin-auto-unpack-natives": "^7.10.2",
    "@electron-forge/plugin-fuses": "^7.10.2",
    "@electron-forge/plugin-vite": "^7.10.2",
    "@electron/fuses": "^1.8.0",
    "@eslint/js": "^9.39.1",
    "@types/crypto-js": "^4.2.2",
    "@typescript-eslint/eslint-plugin": "^8.48.1",
    "@typescript-eslint/parser": "^8.48.1",
    "@vitejs/plugin-vue": "^6.0.2",
    "electron": "^39.2.6",
    "electron-builder": "^26.0.0",
    "eslint": "^9.39.1",
    "eslint-plugin-import": "^2.32.0",
    "terser": "^5.44.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.48.1",
    "vite": "^7.2.6",
    "vue-tsc": "3.1.5"
  },
  "dependencies": {
    "@types/pdfkit": "^0.17.4",
    "axios": "^1.13.2",
    "bottleneck": "^2.19.5",
    "crypto-js": "^4.2.0",
    "driver.js": "^1.4.0",
    "electron-store": "^11.0.2",
    "ffmpeg-static": "^5.3.0",
    "hls.js": "^1.6.15",
    "pdfkit": "^0.17.2",
    "sharp": "^0.34.5",
    "vue": "^3.5.25",
    "vue-i18n": "^11.2.2"
  }
}
